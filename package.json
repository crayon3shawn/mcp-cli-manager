{
  "name": "mcp-cli-manager",
  "version": "1.0.6",
  "description": "A simple CLI tool for managing MCP servers",
  "type": "module",
  "main": "dist/bin/mcp-cli-manager.js",
  "bin": {
    "mcp": "./dist/bin/mcp-cli-manager.js"
  },
  "scripts": {
    "dev": "ts-node src/bin/mcp-cli-manager.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "link": "npm run build && npm link",
    "build": "tsc",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply ."
  },
  "keywords": [
    "mcp",
    "cli",
    "server",
    "manager",
    "model-context-protocol"
  ],
  "author": "crayon3shawn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crayon3shawn/mcp-cli-manager.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "conf": "^12.0.0",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.4.0",
    "inquirer": "^9.2.0",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "npm": "^10.0.0",
    "npm-package-arg": "^10.0.0",
    "npm-registry-fetch": "^14.0.0",
    "ora": "^8.0.0",
    "semver": "^7.5.0",
    "tree-kill": "^1.2.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/cross-spawn": "^6.0.6",
    "@types/inquirer": "^8.2.10",
    "@types/inquirer-autocomplete-prompt": "^3.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.0.9"
  }
}
